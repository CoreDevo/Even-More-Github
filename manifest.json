{
  "name": "Even More Github",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Enhance your Github experience",
  "homepage_url": "http://coredevo.wordpress.com",
  "permissions": [
    "storage"
  ],
  "background": {
    "page": "src/html/background.html"
  },
  "icons": {
    "48": "src/icons/icon48.png",
    "128": "src/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/"],
      "js": ["src/js/token.js","src/js/jquery-2.1.1.min.js","src/js/main.js"],
      "css": ["src/css/style.css"]
    }
 ]
}